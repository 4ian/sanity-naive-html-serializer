{
  "compilerOptions": {
    "incremental": true,
    "target": "es5", 
    "module": "ES2015",                           
    "moduleResolution": "node",
    "strict": true,                               
    "esModuleInterop": true,                      
    "skipLibCheck": true,                         
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true
  }
}
